{
    "name": "veseluy-rodjer/laravel-builder-generator",
    "description": "Generate Laravel builders",
    "keywords": ["generator", "php", "cli", "generator", "laravel", "artisan"],
    "homepage": "https://github.com/veseluy-rodjer/laravel-builder-generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Veseluy Rodjer"
        }
